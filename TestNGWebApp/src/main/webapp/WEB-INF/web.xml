<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">
  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <display-name>HelloServlet</display-name>
    <servlet-class>org.testngwebrunner.app.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/HelloServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ProjectLoaderServlet</servlet-name>
    <display-name>ProjectLoaderServlet</display-name>
    <servlet-class>org.testngwebrunner.app.ProjectLoaderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProjectLoaderServlet</servlet-name>
    <url-pattern>/ProjectLoaderServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>WebSocketTestServlet</servlet-name>
    <display-name>WebSocketTestServlet</display-name>
    <servlet-class>org.testngwebrunner.app.WebSocketTestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WebSocketTestServlet</servlet-name>
    <url-pattern>/WebSocketTestServlet</url-pattern>
  </servlet-mapping>
</web-app>
